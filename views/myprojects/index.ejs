<% include ../partials/header %>

		<header class="jumbotron">
			<div class="container">
				<!--<h1><span class="glyphicon glyphicon-wrench"></span> Welcome to the RenchTech</h1>-->
				<h1><img src="/media/renchtech_logo_solo_001.png" width="10%" height="10%"> My Projects</h1>
				<p>View some of my projects featuring Arduino, Digispark, Raspberry Pi, and Javascript</p>
			</div>
		</header>

		<div class="row text-center" style="display:flex; flex-wrap: wrap;">
			<div class="col-lg-12">
				<h3>My Projects</h3>
			</div>
			
			<% projects.forEach(function(project) { %>
				<div class="col-md-3 col-sm-6">
					<div class="thumbnail">
						<img src= "<%= project.image %>">
						<div class="caption">
							<h4><%= project.title %></h4>	
						</div>
						<p>
							<a href="/myprojects/<%= project._id %>" class="btn btn-primary">More Info</a>
						</p>
					</div>
				</div>
			<% }); %>
			
		</div>
<% include ../partials/footer %>