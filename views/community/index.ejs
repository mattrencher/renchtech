<% include ../partials/header %>

		<header class="jumbotron">
			<div class="container">
				<!--<h1><span class="glyphicon glyphicon-wrench"></span> Welcome to the RenchTech</h1>-->
				<h1><img src="/media/renchtech_logo_solo_001.png" width="10%" height="10%"> Community Hub</h1>
				<p>Make sure to sign up for an account, and add some of your own projects to our Community Hub!</p>
				<p>
					<a class="btn btn-primary btn-lg" href="/community/new">Add New Project</a>
				</p>
			</div>
		</header>

		<div class="row text-center">
			<!--<div class="col-lg-12">-->
			<!--	<h3>Current Projects</h3>-->
			<!--</div>-->
			
			<% projects.forEach(function(project) { %>
				<div class="col-lg-3 col-md-4 col-sm-6 mb-4">
					<div class="card" >
					  <img class="card-img-top" src="<%= project.image %>" alt="<%= project.name %>">
					  <div class="card-body">
					    <h5 class="card-title"><%= project.name %></h5>
					    <!--<p class="card-text">Some quick example text</p>-->
					    <a href="/community/<%= project._id %>" class="btn btn-primary">More Info</a>
					  </div>
					</div>

				</div>
			<% }); %>
		</div>

<% include ../partials/footer %>