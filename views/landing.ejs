    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    <ul class="nav justify-content-center" id="landing-nav">
      <li class="nav-item">
        <a class="nav-link active" href="/projects">Projects</a>
      </li>
      <li class="nav-item">
        <a class="nav-link active" href="/community">Community</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/login">Login</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/register">Signup</a>
      </li>
    </ul>
    
    <div id="carouselFade" class="carousel slide carousel-fade" data-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img class="d-block w-100" src="/landing/img1.jpg">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/landing/img2.jpg">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/landing/img3.jpg">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/landing/im4.jpg">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/landing/img5.jpg">
        </div>        
      </div>
    </div>
      
